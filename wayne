print("load")
getgenv().myGlobalVariable = {
    ["Magma"] = "579bde1f-35db-4c24-acfa-4ab62259a6a1",
    ["Lunar Egg Spawned"] = "99140dda-9b8a-4d56-b7d2-e92f4b002017"
}

-- Iterate over the table directly
for key, value in pairs(getgenv().myGlobalVariable) do
    print(key, value)  -- Print both the key and the value
    task.wait(0.1)     -- Wait for 0.1 seconds
end
